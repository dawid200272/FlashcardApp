<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <!--Colors-->
    <Color x:Key="ColorPrimaryDark">#353535</Color>
    <Color x:Key="ColorPrimaryLight">#505050</Color>
    
    <!--Brushes-->
    <SolidColorBrush x:Key="BrushPrimaryDark" Color="{StaticResource ColorPrimaryDark}" />
    <SolidColorBrush x:Key="BrushPrimaryLight" Color="{StaticResource ColorPrimaryLight}" />
    
    <!--TextBlock-->
    <Style TargetType="TextBlock">
        <Setter Property="Foreground" Value="White"/>
    </Style>

</ResourceDictionary>